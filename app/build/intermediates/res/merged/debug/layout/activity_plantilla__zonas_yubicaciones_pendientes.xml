<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="com.gruposalinas.elektra.movilidadgs.Activities.Plantilla_ZonasYubicaciones_pendientes">

    <android.support.v4.widget.DrawerLayout
        android:layout_width="match_parent"
        android:id="@+id/drawer_layout"
        android:layout_height="match_parent">

        <FrameLayout
            android:orientation="vertical"
            android:id="@+id/content_frame"
            android:alpha="9"
            android:layout_width="240dp"
            android:layout_height="match_parent" />

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="55dp"
                android:orientation="horizontal"
                android:visibility="visible"
                android:background="#FFD700">
                <LinearLayout
                    android:id="@+id/regresar2"
                    android:layout_width="match_parent"
                    android:layout_height="80dp">
                    <ImageButton
                        android:id="@+id/boton_regresar"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/flecha_cabeza"
                        android:layout_width="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Mis ubicaciones de mi plantilla"
                        android:textColor="#373333"
                        android:gravity="center_horizontal"
                        android:textSize="15dp"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="15dp"
                        android:textAppearance="?android:attr/textAppearanceLarge" />



                </LinearLayout>



            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:layout_marginTop="15dp"
                android:layout_below="@+id/textView4"
                android:layout_alignStart="@+id/textView4"
                android:id="@+id/linearLayout">

                <TextView
                    android:textColor="#373333"
                    android:text="Empleado"
                    android:id="@+id/seleccionEmpleado"
                    android:layout_width="300dp"
                    android:textSize="15dp"
                    android:layout_marginLeft="20dp"
                    android:layout_height="wrap_content">
                </TextView>
                <ImageView
                    android:background="@drawable/flecha_proponer"
                    android:layout_width="20dp"
                    android:layout_height="15dp" />

            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:id="@+id/linea"
                android:layout_marginLeft="20dp"
                android:background="#030303"
                android:layout_width="330dp"
                android:layout_height="1dp">
            </LinearLayout>

            <TextView
                android:layout_marginTop="10dp"
                android:layout_gravity="left"
                android:id="@+id/textozonas"
                android:textSize="15dp"
                android:text="Zonas"
                android:textColor="#666666"

                android:gravity="left"
                android:background="#D3D3D3"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />


            <TextView
                android:id="@+id/mensaje"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="psible texto"
                android:gravity="center"
                android:textSize="17dp"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_centerInParent="true"
                android:layout_marginLeft="15dp"
                android:layout_marginRight="15dp"
                android:layout_marginTop="15dp"
                android:visibility="gone"
                android:layout_gravity="center_horizontal" />

            <RelativeLayout
                android:layout_height="match_parent"
                android:layout_width="match_parent"
                android:gravity="center"
                android:layout_centerInParent="true"
                android:id="@+id/progressbar_tiendas"
                android:visibility="gone" >

                <ImageView
                    android:id="@+id/main_progressbar_imageview_tiendas"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/loading_animation"/>

            </RelativeLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_centerInParent="true"
                android:layout_gravity="center_horizontal">
                <GridView
                    android:minHeight="30dp"
                    android:gravity="center"
                    android:textColor="#666666"
                    android:layout_centerInParent="true"
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/listatiendas"
                    android:layout_width="wrap_content"
                    android:layout_height="150dp">
                </GridView>
            </LinearLayout>
            <ImageView
                android:layout_centerHorizontal="true"
                android:id="@+id/imagenIndicadorzonas"
                android:layout_width="wrap_content"
                android:layout_gravity="center"
                android:visibility="gone"
                android:background="@android:drawable/arrow_down_float"
                android:layout_height="wrap_content" />


            <TextView
                android:layout_gravity="left"
                android:id="@+id/textoubicaciones"
                android:textSize="15dp"
                android:text="Ubicaciones"
                android:textAppearance="?android:attr/textAppearanceLarge"
                android:layout_width="match_parent"
                android:textColor="#666666"
                android:background="#D3D3D3"
                android:layout_height="wrap_content" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_centerInParent="true"
                android:layout_marginTop="1dp"
                android:layout_gravity="center_horizontal">
                <GridView
                    android:minHeight="30dp"
                    android:gravity="center"
                    android:layout_centerInParent="true"
                    android:layout_gravity="center_horizontal"
                    android:id="@+id/listatiendasUbicaciones"
                    android:layout_width="wrap_content"
                    android:textColor="#666666"
                    android:layout_height="160dp">
                </GridView>
            </LinearLayout>

            <LinearLayout
                android:id="@+id/tiendas_pendientes"
                android:layout_width="350dp"
                android:layout_height="50dp"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_centerInParent="true"
                android:background="#DD5F19"
                android:visibility="invisible"
                android:layout_marginTop="1dp"
                android:layout_gravity="center_horizontal">
                <TextView
                    android:text="Zonas y Ubicaciones pendientes de autorizar"
                    android:layout_width="wrap_content"
                    android:textColor="#FFFFFF"
                    android:layout_gravity="center"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:layout_height="wrap_content" />
            </LinearLayout>



            <LinearLayout
                android:layout_below="@+id/listatiendasUbicaciones"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:background="#3F3E3E"
                android:layout_marginTop="8dp"
                android:layout_gravity="center"
                android:gravity="center_horizontal"
                android:layout_height="match_parent">


                <ImageView
                    android:id="@+id/paginaweb"
                    android:gravity="center"
                    android:textSize="15dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="1dp"
                    android:background="@drawable/cuentanos"
                    android:layout_marginLeft="140dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:layout_width="25dp"
                    android:layout_height="25dp" />

                <LinearLayout
                    android:visibility="visible"
                    android:id="@+id/panico"
                    android:layout_width="40dp"
                    android:layout_marginLeft="120dp"
                    android:layout_height="60dp">

                    <ImageView
                        android:background="@drawable/panico_icono"
                        android:layout_width="25dp"
                        android:layout_marginTop="16dp"
                        android:layout_height="25dp" />


                </LinearLayout>


            </LinearLayout>

        </LinearLayout>


        <FrameLayout
            android:layout_width="240dp"
            android:layout_gravity="start"
            android:id="@+id/contenidor"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="100dp"
                android:layout_height="100dp">

                <ImageView
                    android:id="@+id/dlete"
                    android:background="@drawable/boton_cerrar_x2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

            </LinearLayout>


            <FrameLayout
                android:layout_marginTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">


                <LinearLayout
                    android:id="@+id/horarios_menu"
                    android:layout_marginTop="50dp"
                    android:layout_width="300dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="70dp">

                    <ImageView
                        android:id="@+id/reloj"
                        android:background="@drawable/horario3"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="30dp"
                        android:layout_height="30dp" />


                    <TextView
                        android:id="@+id/texto_menuHorario"
                        android:text="Horarios"
                        android:layout_marginTop="15dp"
                        android:layout_width="wrap_content"
                        android:layout_marginRight="10dp"
                        android:textColor="#EFEEEE"
                        android:textSize="18sp"
                        android:layout_height="wrap_content" />


                </LinearLayout>


                <LinearLayout
                    android:id="@+id/Ubicacion_menu"
                    android:layout_marginTop="115dp"
                    android:layout_width="300dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="70dp">

                    <ImageView
                        android:id="@+id/tocar"
                        android:background="@drawable/ubicaciones3"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="30dp"
                        android:layout_height="30dp" />


                    <TextView
                        android:id="@+id/texto_menuUbicacion"
                        android:text="Ubicaciones"
                        android:layout_marginTop="15dp"
                        android:layout_width="wrap_content"
                        android:layout_marginRight="10dp"
                        android:textColor="#EFEEEE"
                        android:textSize="18sp"
                        android:layout_height="wrap_content" />

                </LinearLayout>

                <LinearLayout
                    android:visibility="invisible"
                    android:id="@+id/menu_incidencias"
                    android:layout_marginTop="190dp"
                    android:layout_width="300dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="70dp">

                    <ImageView
                        android:id="@+id/icono_incidenciasmenu"
                        android:background="@drawable/incidencias_icono"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="30dp"
                        android:layout_height="30dp" />


                    <TextView
                        android:id="@+id/texto_menuIncidencias"
                        android:text="Incidencias"
                        android:layout_marginTop="15dp"
                        android:layout_width="wrap_content"
                        android:layout_marginRight="10dp"
                        android:textColor="#EFEEEE"
                        android:textSize="18sp"
                        android:layout_height="wrap_content" />


                </LinearLayout>


                <LinearLayout
                    android:visibility="visible"
                    android:id="@+id/contacto"
                    android:layout_marginTop="190dp"
                    android:layout_width="300dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="70dp">

                    <ImageView
                        android:id="@+id/contacto1"
                        android:background="@drawable/icono_contacto_principal"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="30dp"
                        android:layout_height="30dp" />


                    <TextView
                        android:id="@+id/contactotexto"
                        android:text="Aseguradoras"
                        android:layout_marginTop="15dp"
                        android:layout_width="wrap_content"
                        android:layout_marginRight="10dp"
                        android:textColor="#EFEEEE"
                        android:textSize="18sp"
                        android:layout_height="wrap_content" />


                </LinearLayout>


                <LinearLayout
                    android:visibility="visible"
                    android:id="@+id/menu_panico"
                    android:layout_marginTop="270dp"
                    android:layout_width="300dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="70dp">

                    <ImageView
                        android:id="@+id/icono_panico_menu"
                        android:background="@drawable/icono_panico"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="30dp"
                        android:layout_height="30dp" />


                    <TextView
                        android:id="@+id/texto_menupanico"
                        android:text="Bóton de Pánico"
                        android:layout_marginTop="15dp"
                        android:layout_width="wrap_content"
                        android:layout_marginRight="10dp"
                        android:textColor="#EFEEEE"
                        android:textSize="18sp"
                        android:layout_height="wrap_content" />

                </LinearLayout>

                <LinearLayout
                    android:visibility="gone"
                    android:id="@+id/pendientes_autorizar"
                    android:layout_marginTop="340dp"
                    android:layout_width="300dp"
                    android:layout_marginLeft="10dp"
                    android:layout_height="70dp">

                    <ImageView
                        android:id="@+id/icono_pendiente"
                        android:background="@drawable/icono_panico"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="10dp"
                        android:layout_width="30dp"
                        android:layout_height="30dp" />


                    <TextView
                        android:id="@+id/texto_pendiente"
                        android:text="Pendientes Autorizar"
                        android:layout_marginTop="15dp"
                        android:layout_width="wrap_content"
                        android:layout_marginRight="10dp"
                        android:textColor="#EFEEEE"
                        android:textSize="18sp"
                        android:layout_height="wrap_content" />


                </LinearLayout>


            </FrameLayout>
        </FrameLayout>


    </android.support.v4.widget.DrawerLayout>

</android.support.v4.widget.DrawerLayout>
